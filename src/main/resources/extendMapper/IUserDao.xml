<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="extend.use.dao.IUserDao">
    <!--配置查询所有-->
    <select id="findAll" resultType="extend.use.domain.User">
        select * from user
    </select>

    <select id="getUserById" resultType="extend.use.domain.User">
        select * from user where id = #{id}
    </select>
</mapper>